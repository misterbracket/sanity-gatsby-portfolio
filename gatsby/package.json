{
  "private": true,
  "name": "sanity-gatsby-portfolio-web",
  "version": "1.0.2",
  "author": "Max Klammer",
  "scripts": {
    "build": "NODE_OPTIONS=\"-r esm\" gatsby build",
    "clean-cache": "gatsby clean",
    "develop": " npm run clean-cache && NODE_OPTIONS=\"-r esm\" gatsby develop --port 8001",
    "start": " npm run develop",
    "lint": "eslint .",
    "format": "prettier-eslint --write \"**/*.js\" \"!.cache/**\" \"!node_modules/**\" \"!public/**\"",
    "test": "echo \"Write tests! -> https://gatsby.app/unit-testing\"",
    "netlify": "netlify dev",
    "prebuild": "netlify-lambda install"
  },
  "eslintConfig": {
    "extends": [
      "wesbos"
    ],
    "rules": {
      "react/prop-types": 0
    }
  },
  "dependencies": {
    "@fontsource/poppins": "^4.1.0",
    "@sanity/block-content-to-react": "^2.0.7",
    "@sanity/image-url": "^0.140.18",
    "gatsby": "^2.30.2",
    "gatsby-image": "^2.9.0",
    "gatsby-plugin-codegen": "^1.2.14",
    "gatsby-plugin-postcss": "^2.0.7",
    "gatsby-plugin-react-helmet": "^3.8.0",
    "gatsby-plugin-styled-components": "^3.10.0",
    "gatsby-plugin-typescript": "^2.10.0",
    "gatsby-source-sanity": "^4.0.1",
    "normalize.css": "^8.0.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-helmet": "^5.2.0",
    "react-icons": "^4.1.0",
    "react-use": "^15.3.8",
    "styled-components": "^5.2.1"
  },
  "devDependencies": {
    "@sanity/types": "^2.2.6",
    "@types/node": "^14.14.21",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-helmet": "^6.1.0",
    "@types/styled-components": "^5.1.7",
    "babel-plugin-styled-components": "^1.12.0",
    "dotenv": "^8.2.0",
    "eslint": "^5.16.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-config-standard-react": "^7.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^9.2.0",
    "eslint-plugin-promise": "^4.1.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-standard": "^4.1.0",
    "esm": "^3.2.25",
    "postcss-import": "^12.0.1",
    "postcss-preset-env": "^6.6.0",
    "prettier-eslint-cli": "^4.7.1",
    "rimraf": "^2.7.1",
    "typescript": "^4.1.3",
    "typescript-plugin-styled-components": "^1.4.4"
  }
}
