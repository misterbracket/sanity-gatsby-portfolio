    "netlify-lambda": "^2.0.3",
    "normalize.css": "^8.0.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-helmet": "^6.1.0",
    "react-icons": "^4.1.0",
    "react-use": "^17.2.1",
    "styled-components": "^5.2.1"
