{
  "private": true,
  "name": "sanity-gatsby-portfolio-web",
  "version": "1.0.2",
  "author": "Max Klammer",
  "scripts": {
    "build": "NODE_OPTIONS=\"-r esm\" gatsby build",
    "clean-cache": "gatsby clean",
    "develop": "NODE_OPTIONS=\"-r esm\" gatsby develop --port 8001",
    "start": " npm run develop",
    "lint": "eslint .",
    "format": "prettier-eslint --write \"**/*.js\" \"!.cache/**\" \"!node_modules/**\" \"!public/**\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "netlify": "netlify dev",
    "prebuild": "netlify-lambda install"
  },
  "eslintConfig": {
    "extends": [
      "wesbos",
      "testing-library",
      "jest-dom"
    ],
    "rules": {
      "react/prop-types": 0
    }
  },
  "dependencies": {
